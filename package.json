{
  "name": "geomoment-angular",
  "version": "0.1.2",
  "description": "An angular module for date formatting; wraps geomoment. For use with browserify.",
  "main": "lib/index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha --compilers coffee:coffee-script spec/*.spec.coffee",
    "pretest": "./node_modules/.bin/coffee --bare --compile --output lib/ src/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/goodeggs/geomoment-angular.git"
  },
  "keywords": [
    "moment",
    "date",
    "time",
    "timezone",
    "tz",
    "moment-timezone",
    "browserify",
    "angular.js"
  ],
  "author": "Max Edmands <git@maxedmands.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/goodeggs/geomoment-angular/issues"
  },
  "dependencies": {
    "geomoment": ">=0.2.1"
  },
  "devDependencies": {
    "mocha": "~1.15.1",
    "coffee-script": "~1.6.3",
    "chai": "~1.8.1"
  }
}
